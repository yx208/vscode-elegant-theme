{
	"$schema": "vscode://schemas/color-theme",
	"name": "Elegant Theme",
	"colors": {
		"editor.background": "#1A1C1F",
		"editor.foreground": "#A9B7C6",

		"editorGroupHeader.tabsBackground": "#1E2024",

		"editorCursor.foreground": "#2075EB",
		"activityBarBadge.background": "#007acc",
		
		"terminal.background": "#1E2024",

		"sideBar.background": "#1E2024",
		"sideBar.border": "#373B42",

		"tab.inactiveBackground": "#1E2024",
		"tab.activeBorderTop": "#2075EB",
		"tab.border": "#373B42",

		"statusBar.background": "#1E2024",

		"activityBar.background": "#1E2024",
		"activityBar.border": "#373B42",

		"debugToolBar.background": "#1E2024",

		"titleBar.activeBackground": "#1E2024",
		"titleBar.border": "#373B42",

		"sideBarSectionHeader.background": "#1E2024",
	},
	"tokenColors": [
		{
			"name": "Class propertices/",
			"scope": [
				"meta.field.declaration.ts",
				"keyword.operator.type.annotation.ts",
				"meta.objectliteral.ts"
			],
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "separator terminator",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#5B697C",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#008053"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#009963"
			}
		},
		{
			"name": "Class",
			"scope": [
				"storage.class",
				"support.class",
				"meta.interface",
				"entity.name.type",
				"entity.name.namespace"
			],
			"settings": {
				"foreground": "#00B899"
			}
		},
		// {
		// 	"name": "Type/ts",
		// 	"scope": [
		// 		"support.type",
		// 		"entity.name.type.interface.ts"
		// 	],
		// 	"settings": {
		// 		"foreground": "#00B899",
		// 		"fontStyle": "italic"
		// 	}
		// },
		{
			"name": "html/TSX/JSX/CustomComponent Tag Name",
			"scope": [
				"entity.name.tag.tsx",
				"entity.name.tag.jsx",
				"support.class.component.tsx",
    			"support.class.component.jsx",
				"entity.name.tag.html",
				// "entity.name.tag.structure.any.html",
				// "entity.name.tag.inline.any.html"
			],
			"settings": {
				"foreground": "#4997FA",
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#4997FA"
			}
		},
		{
			"name": "Function Argument",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#49B3F5"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.type.interface.ts",
				"storage.modifier.ts",
				"storage.type.class",
				"storage.type.ts",
				"storage.modifier.async.ts",
				"meta.type.annotation.ts",
				"variable.language.this",
				"variable.language.self.rust"
			],
			"settings": {
				"foreground": "#E0992D"
			}
		},
		{
			"name": "Number, Constant, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape"
			],
			"settings": {
				"foreground": "#EB8800"
			}
		},
		{
			"name": "other variable",
			"scope": [
				"meta.var-single-variable.expr.ts"
			],
			"settings": {
			  "foreground": "#9581D6"
			}
		},
		{
			"name": "RW gloabl var",
			"scope": [
				"variable.other.object.ts",
				"variable.other.readwrite.alias.ts"
			],
			"settings": {
				"foreground": "#E66060"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"class": "#00B899",
		"keyword": "#E0992D"
	}
}
