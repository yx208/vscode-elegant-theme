{
	"$schema": "vscode://schemas/color-theme",
	"name": "Elegant Theme",
	"colors": {
		"editor.background": "#1A1C1F",
		"editor.foreground": "#A9B7C6",

		"editorLineNumber.foreground": "#41484D",
		"editorLineNumber.activeForeground": "#A9B7C6",

		"editorGroupHeader.tabsBackground": "#1f2226",

		"editorCursor.foreground": "#2075EB",
		
		"terminal.background": "#1A1C1F",

		"sideBar.background": "#1f2226",
		"sideBar.border": "#373B42",

		"tab.inactiveBackground": "#1f2226",
		"tab.activeBorderTop": "#2075EB",
		"tab.border": "#373B42",

		"statusBar.background": "#1f2226",
		"statusBar.border": "#373B42",

		"activityBar.background": "#1f2226",
		"activityBar.border": "#373B42",

		"debugToolBar.background": "#1f2226",

		"titleBar.activeBackground": "#1f2226",
		"titleBar.border": "#373B42",

		"sideBarSectionHeader.background": "#1f2226",

		"welcomePage.background": "#1A1C1F",

		"menu.background": "#1f2226",
		"menu.border": "#373B42",
		"menu.separatorBackground": "#373B42",
		"menu.selectionBackground": "#0F62CC"
	},
	"tokenColors": [
		{
			"name": "Text",
			"scope": [
				"meta.field.declaration",
				"keyword.operator.type.annotation",
				"keyword.operator.assignment",
			],
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "Constant Property",
			"scope": [
				"variable.other.constant.property"
			],
			"settings": {
				"foreground": "#A9B7C6",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "TS:Text",
			"scope": [
				"meta.objectliteral.ts",
				"variable.other.object.property.ts"
			],
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "JSON:Field name",
			"scope": ["support.type.property-name.json"],
			"settings": {
				"foreground": "#9DA4AF"
			}
		},
		{
			"name": "separator terminator",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#5B697C",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#008053"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#009963"
			}
		},
		{
			"name": "Class",
			"scope": [
				"storage.class",
				"support.class",
				"meta.interface",
				"entity.name.type",
				"entity.name.namespace"
			],
			"settings": {
				"foreground": "#00B899"
			}
		},
		{
			"name": "html/TSX/JSX/CustomComponent:Tag Name",
			"scope": [
				"entity.name.tag.tsx",
				"entity.name.tag.jsx",
				"support.class.component.tsx",
    			"support.class.component.jsx",
				"entity.name.tag.html"
			],
			"settings": {
				"foreground": "#4997FA",
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"name.function",
				"variable.function",
				"support.function"
			],
			"settings": {
				"foreground": "#4997FA"
			}
		},
		{
			"name": "Log:Contant",
			"scope": [
				"log.constant"
			],
			"settings": {
				"foreground": "#4997FA"
			}
		},
		{
			"name": "Function Argument",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#49B3F5"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.type.class",
				"variable.language.this",
				"entity.name.type.parameter"
			],
			"settings": {
				"foreground": "#E0992D"
			}
		},
		{
			"name": "Rust:Self",
			"scope": ["variable.language.self.rust"],
			"settings": {
				"foreground": "#E0992D"
			}
		},
		{
			"name": "TS:Keyword",
			"scope": [
				"storage.type.interface.ts",
				"storage.type.interface.tsx",
				"storage.type.function.tsx",
				"storage.modifier.ts",
				"storage.type.ts",
				"storage.type.tsx",
				"storage.modifier.async.ts",
				"meta.type.annotation.ts",
				"support.type.primitive.ts",
				"support.type.primitive.tsx",
				"keyword.control.import.tsx",
				"keyword.control.import.ts",
				"keyword.control.export.tsx",
				"keyword.control.export.ts",
				"keyword.control.default.tsx",
				"keyword.control.default.ts",
				"keyword.control.flow.tsx",
				"keyword.control.flow.ts"
			],
			"settings": {
				"foreground": "#E0992D",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Number, Constant, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape"
			],
			"settings": {
				"foreground": "#EB8800"
			}
		},
		{
			"name": "Log:Warn",
			"scope": ["log.warning"],
			"settings": {
				"foreground": "#EB8800"
			}
		},
		{
			"name": "Other variable",
			"scope": [],
			"settings": {
			  "foreground": "#9581D6"
			}
		},
		{
			"name": "Log:Date",
			"scope": ["log.date"],
			"settings": {
			  "foreground": "#9581D6"
			}
		},
		{
			"name": "RW gloabl var",
			"scope": [],
			"settings": {
				"foreground": "#E66060"
			}
		},
		{
			"name": "Log:Error",
			"scope": ["log.error"],
			"settings": {
				"foreground": "#E66060"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"class": "#00B899",
		"keyword": "#E0992D"
	}
}
